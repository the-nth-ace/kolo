{
    "coverageProvider": "v8",
    "moduleDirectories": ["node_modules", "src"],
    "testEnvironment": "node",
    "transform": {
        "^.+\\.ts?$": "ts-jest"
    },

        "moduleNameMapper": {
            "^@data/(.*)$": "<rootDir>/src/data/$1",
            "^@web/(.*)$": "<rootDir>/src/web/$1",
            "^@logic/(.*)$": "<rootDir>/src/logic/$1"

        }

}